#include <iostream>
using namespace std;
 
int main() {
    int ageInDays;
    cin >> ageInDays;
    int years = ageInDays/365;
    int remaining_days = ageInDays % 365;
    int months = remaining_days/30;
    int days = remaining_days % 30;
    
    cout << years << " ano(s)" << endl;
    cout << months << " mes(es)" << endl;
    cout << days << " dia(s)" << endl;
    
   
    return 0;
}
