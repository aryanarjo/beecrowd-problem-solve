#include <iostream>
using namespace std;
 
int main() {
    int number[5];
    int evenNumber = 0;
    for (int i = 0; i < 5; i++) {
        cin >> number[i];
        if (number[i] % 2 == 0) {
            evenNumber++;
        }
    }
    cout << evenNumber << " valores pares" << endl;
    
    return 0;
}
